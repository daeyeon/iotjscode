<!-- Backtrace -->
<div class="row vertical-resizable" id="backtrace-wrapper">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <span>Backtrace</span>
      </div>
      <div class="panel-body backtrace">
        <div class="wrapper">
          <table class="table table-striped table-hover scroll-table" id="backtrace-table">
            <thead>
              <tr>
                <th width="70">Frame</th>
                <th>Resource</th>
                <th width="70">Line</th>
                <th>Function</th>
              </tr>
            </thead>
            <tbody id="backtrace-table-body"></tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>

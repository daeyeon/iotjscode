<!-- Breakpoint list -->
<div class="row vertical-resizable" id="breakpoints-wrapper">
  <div class="col-md-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <span>Breakpoints</span>
        <div class="btn btn-default debugger-action-button btn-xs disabled" id="delete-all-button" title="Delete all breakpoint">Delete all</div>
      </div>
      <div class="panel-body breakpoints" id="breakpoints-body">
        <div class="wrapper">
          <table class="table table-striped table-hover scroll-table" id="breakpoints-table">
            <thead>
              <tr>
                <th>Resource</th>
                <th width="70">Line</th>
                <th width="70">ID</th>
                <th>Function</th>
              </tr>
            </thead>
            <tbody id="breakpoints-table-body"></tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
